<h3 class="movie-list-title">Liste des films r√©cent</h3>
<div class="movie-list-container">
	<% mostRecentMovie.forEach((mostRecentMovie) => { %>
	<div class="fresh-movie-container">
		<a href="<%= mostRecentMovie.url %>">
			<img src="/img/<%= mostRecentMovie.image %>" alt="<%mostRecentMovie.name%>" loading="fast" />
			<h2><%= mostRecentMovie.name %></h2>
		</a>
	</div>
	<%})%>
</div>

